{
  "name": "flexio",
  "version": "1.22.0",
  "description": "Flex.io Client Web Application",
  "author": "David Z. Williams <dave@flex.io>",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.dev.js --open --inline --https --hot",
    "build:signup:debug": "cross-env NODE_ENV=debug webpack --config build/webpack.signup.dist.js --progress --hide-modules",
    "build:signup:release": "cross-env NODE_ENV=production webpack --config build/webpack.signup.dist.js --progress --hide-modules",
    "build:debug": "cross-env NODE_ENV=debug webpack --config build/webpack.dist.js --progress --hide-modules",
    "build:release": "cross-env NODE_ENV=production webpack --config build/webpack.dist.js --progress --hide-modules",
    "deploy": "npm run build:release && npm run build:signup:release"
  },
