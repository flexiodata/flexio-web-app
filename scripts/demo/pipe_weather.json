          { "path": "https://raw.githubusercontent.com/fivethirtyeight/data/master/us-weather-history/KCLT.csv" },
          { "path": "https://raw.githubusercontent.com/fivethirtyeight/data/master/us-weather-history/KCQT.csv" },
