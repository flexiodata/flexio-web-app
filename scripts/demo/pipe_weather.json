          { "path": "https://raw.githubusercontent.com/fivethirtyeight/data/master/us-weather-history/KJAX.csv" },
          { "path": "https://raw.githubusercontent.com/fivethirtyeight/data/master/us-weather-history/KMDW.csv" },
          { "path": "https://raw.githubusercontent.com/fivethirtyeight/data/master/us-weather-history/KNYC.csv" },
          { "path": "https://raw.githubusercontent.com/fivethirtyeight/data/master/us-weather-history/KPHL.csv" },
          { "path": "https://raw.githubusercontent.com/fivethirtyeight/data/master/us-weather-history/KPHX.csv" },
