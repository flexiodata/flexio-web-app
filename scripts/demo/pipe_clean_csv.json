{
    "eid_type": "PIP",
    "name": "Clean a CSV",
    "alias": "clean-csv",
    "description": "Clean a CSV by removing some columns",
    "task": {
        "op": "sequence",
        "params": {
            "items": [
                {
                    "op": "request",
                    "params": {
                        "url": "https://raw.githubusercontent.com/flexiodata/data/master/contact-samples/contacts-ltd1.csv"
                    }
                },
                {
                    "op": "convert",
                    "params": {
                        "input": "csv",
                        "output": "table"
                    }
                },
                {
                    "op": "select",
                    "params": {
                        "columns": [
                            "streetaddress",
                            "city",
                            "state",
                            "zipcode"
                        ]
                    }
                }
            ]
        }
    }
}
