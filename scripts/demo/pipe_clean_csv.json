{
    "eid_type": "PIP",
    "name": "Clean a CSV",
    "alias": "clean-csv",
    "description": "Clean a CSV by removing some columns",
    "task": {
        "op": "sequence",
        "items": [
            {
                "op": "request",
                "url": "https://raw.githubusercontent.com/flexiodata/data/master/contact-samples/contacts-ltd1.csv"
            },
            {
                "op": "convert",
                "input": "csv",
                "output": "table"
            },
            {
                "op": "select",
                "columns": [
                    "streetaddress",
                    "city",
                    "state",
                    "zipcode"
                ]
            }
        ]
    }
}
