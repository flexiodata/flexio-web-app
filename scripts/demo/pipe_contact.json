{
    "name": "Contact Refinement",
    "description": "Select a subset of a list of contacts",
    "task": [
        {
            "eid": "ddvwfjzwblc2",
            "type": "flexio.input",
            "params": {
                "items": [
                    {
                        "path": "https:\/\/raw.githubusercontent.com\/flexiodata\/sample-data\/master\/contacts-2016-11.csv"
                    }
                ]
            }
        },
        {
            "eid": "kg0lb3l7vq14",
            "type": "flexio.convert",
            "params": {
                "input": {
                    "format": "delimited",
                    "delimiter": "{comma}",
                    "header": true
                },
                "output": {
                    "format": "table"
                }
            }
        },
        {
            "type": "flexio.select",
            "params": {
                "columns": [
                    "givenname",
                    "surname",
                    "streetaddress",
                    "city",
                    "state",
                    "zipcode"
                ]
            },
            "eid": "dnmtdvx566j6"
        },
        {
            "type": "flexio.transform",
            "params": {
                "columns": [
                    "givenname",
                    "surname",
                    "streetaddress",
                    "city"
                ],
                "operations": [
                    {
                        "operation": "case",
                        "case": "upper"
                    }
                ]
            }
        }
    ]
}
